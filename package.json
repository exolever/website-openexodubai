    "@emotion/styled": "^10.0.14",
    "babel-plugin-styled-components": "^1.10.6",
    "classnames": "^2.2.6",
    "form-serialize": "^0.7.2",
    "gatsby": "^2.13.1",
    "gatsby-cli": "^2.7.7",
    "gatsby-image": "^2.2.4",
    "gatsby-plugin-canonical-urls": "^2.1.0",
    "gatsby-plugin-emotion": "^4.1.0",
    "gatsby-plugin-google-analytics": "^2.1.1",
    "gatsby-plugin-purgecss": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.2",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-prismjs": "^3.3.1",
    "gatsby-remark-responsive-iframe": "^2.2.1",
    "gatsby-remark-smartypants": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-transformer-json": "^2.2.0",
    "gatsby-transformer-remark": "^2.5.0",
    "gatsby-transformer-sharp": "^2.2.1",
    "jsonp": "^0.2.1",
    "modern-normalize": "^0.5.0",
    "normalize.css": "^8.0.0",
    "now": "^15.6.2",
    "polished": "^2.0.3",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.1",
    "react-hotjar": "^1.0.11",
    "typescript": "^3.5.2"
