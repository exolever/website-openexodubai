{
  "name": "site_openexodubai",
  "description": "Exo Summit static website",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "now && now alias",
    "dev": "gatsby develop",
    "now-build": "npm run build",
    "format": "prettier --semi --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "deploy:gh-pages": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
